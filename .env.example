###############################################
# Environment Variables for base-miniapp
# Copy this file to .env and fill in the values
###############################################

# Private key of the deployer account (WITHOUT surrounding quotes).
# Recommended: use a dedicated test wallet. Never commit the real value.
PRIVATE_KEY=

# Optional: Override Base Sepolia RPC URL (defaults to https://sepolia.base.org)
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# (Optional) Local Hardhat network private key if you want to pre-fund/use a custom one
# LOCAL_PRIVATE_KEY=

# Frontend variable (can be filled manually if not using the auto-writer in deploy script)
# This is not required if you rely on frontend/contractInfo.json generated automatically.
NEXT_PUBLIC_CONTRACT_ADDRESS=

# Add any other custom variables below

